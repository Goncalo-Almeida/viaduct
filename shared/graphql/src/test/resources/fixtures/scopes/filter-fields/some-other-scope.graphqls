schema {
  query: Query
}

type AType {
  foo: String
  someOtherTypeField: SomeOtherType
}

"this type will be completely filtered out for all scopes except \"some-other-scope\""
type BType {
  someOtherTypeField: SomeOtherType
}

type Query {
  aType: AType
  bType: BType
}

type SomeOtherType {
  bar: String
}
