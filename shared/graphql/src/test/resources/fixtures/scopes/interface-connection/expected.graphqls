schema {
  query: Query
}

interface Node {
  id: ID!
}

type Foo {
  foo: String
}

type PlaceListing implements Node {
  id: ID!
  presentation: PlaceListingPresentationContainer
}

type PlaceListingPresentationContainer {
  data: PlaceListing
  foo: String
  query: Query
}

type Query {
  foo: Foo
  node: Node
}
